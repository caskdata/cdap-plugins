{
  "metadata": {
    "spec-version": "1.0"
  },
  "configuration-groups": [
    {
      "label": "CopybookReader Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        },
        {
          "widget-type": "textarea",
          "label": "COBOL Copybook Contents",
          "name": "copybookContents"
        },
        {
          "widget-type": "textbox",
          "label": "BinaryFile Path",
          "name": "binaryFilePath"
        },
        {
          "widget-type": "textbox",
          "label": "Maximum Split Size",
          "name": "maxSplitSize"
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "schema",
      "widget-type": "schema",
      "widget-attributes": {
        "schema-types": [
          "boolean",
          "int",
          "long",
          "float",
          "double",
          "string"
        ],
        "schema-default-type": "string"
      }
    }
  ]
}
