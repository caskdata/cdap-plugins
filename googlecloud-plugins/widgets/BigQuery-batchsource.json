{
  "metadata": {
    "spec-version": "1.0"
  },
  "configuration-groups": [
    {
      "label": "BigQuery",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        },
        {
          "widget-type": "textbox",
          "label": "Project ID",
          "name": "projectId"
        },
        {
          "widget-type": "textbox",
          "label": "JSON Key File Path",
          "name": "jsonKeyFilePath"
        },
        {
          "widget-type": "textbox",
          "label": "Temporary Google Storage Path",
          "name": "tempBucketPath"
        },
        {
          "widget-type": "textbox",
          "label": "Input Table",
          "name": "inputTable"
        },
        {
          "widget-type": "textbox",
          "label": "Intermediate Table",
          "name": "intermediateTable"
        },
        {
          "widget-type": "textarea",
          "label": "Import Query",
          "name": "importQuery",
          "widget-attributes": {
            "rows": "4"
          }
        }
      ]
    }
  ],
  "outputs": [
    {
      "name": "outputSchema",
      "widget-type": "schema",
      "widget-attributes": {
        "schema-types": [
          "boolean",
          "int",
          "long",
          "float",
          "double",
          "bytes",
          "string"
        ],
        "schema-default-type": "string"
      }
    }
  ]
}
